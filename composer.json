{
    "name": "rmxmedia/laravel-code-utils",
    "description": "Code utilities (analysis, coding-standard, ...) for laravel projects",
    "version": "1.0.3",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.2",
        "symplify/easy-coding-standard": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.4"
    },
    "bin": [
        "bin/ecs",
        "bin/php-cs-fixer",
        "bin/phpstan",
        "bin/phpstan.phar"
    ]
}
